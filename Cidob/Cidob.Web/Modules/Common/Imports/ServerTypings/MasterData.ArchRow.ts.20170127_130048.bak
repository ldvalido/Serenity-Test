
namespace Cidob.MasterData {
    export interface ArchRow {
        Id?: number;
        Product?: string;
        Order?: number;
        Description?: string;
        PrintDescription?: string;
    }

    export namespace ArchRow {
        export const idProperty = 'Id';
        export const nameProperty = 'Product';
        export const localTextPrefix = 'MasterData.Arch';

        export namespace Fields {
            export declare const Id;
            export declare const Product;
            export declare const Order;
            export declare const Description;
            export declare const PrintDescription;
        }

        ['Id', 'Product', 'Order', 'Description', 'PrintDescription'].forEach(x => (<any>Fields)[x] = x);
    }
}

